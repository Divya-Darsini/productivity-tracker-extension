{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "A productivity tracker extension built with MERN.",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
